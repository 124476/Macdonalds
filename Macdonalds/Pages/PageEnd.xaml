<Page x:Class="Macdonalds.Pages.PageEnd"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Macdonalds.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageEnd">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="200">
            <TextBlock Text="Ваш номер:" FontSize="27" HorizontalAlignment="Center"></TextBlock>
            <TextBlock x:Name="NumberText" FontSize="25" HorizontalAlignment="Center"></TextBlock>
            <Button x:Name="BackBtn" Content="На главную" Click="BackBtn_Click" Style="{StaticResource StandartButton}" Margin="0,100,0,0"></Button>
        </StackPanel>
    </Grid>
</Page>
